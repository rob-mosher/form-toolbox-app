{
  "name": "form-toolbox-app-api",
  "private": true,
  "version": "0.0.1",
  "description": "Form Toolbox API service",
  "scripts": {
    "build": "tsc",
    "check:types": "npx tsc --noEmit --skipLibCheck",
    "dev": "API_HOST=127.0.0.1 nodemon --exec ts-node src/index.ts",
    "dev:docker": "API_HOST=0.0.0.0 nodemon --exec ts-node src/index.ts",
    "lint": "eslint . --ext .js,.ts --report-unused-disable-directives --max-warnings 0",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "start": "API_HOST=0.0.0.0 node dist/index.js",
    "test": "echo \"Error: Please initiate tests via the root of the repository\" && exit 1"
  },
  "author": "",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.429.0",
    "@aws-sdk/client-sqs": "^3.441.0",
    "@aws-sdk/client-textract": "^3.635.0",
    "@aws-sdk/credential-providers": "^3.430.0",
    "@aws-sdk/s3-request-presigner": "^3.450.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-list-endpoints": "^7.1.0",
    "multer": "^1.4.5-lts.1",
    "pdf2pic": "^3.1.3",
    "pg": "^8.11.3",
    "sequelize": "^6.34.0",
    "sharp": "^0.32.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-list-endpoints": "^6.0.3",
    "@types/multer": "^1.4.11",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.51.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "nodemon": "^3.0.1",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
